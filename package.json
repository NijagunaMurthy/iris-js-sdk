        "sdp-interop": "^0.1.11",
        "sdp-transform": "^1.6.2",
        "uuid": "3.0.1"
    },
    "devDependencies": {
        "browserify": "^14.1.0",
        "bufferutil": "^3.0.0",
        "chai": "^3.2.0",
        "chai-http": "^1.0.0",
        "cors": "2.7.1",
        "docdash": "^0.4.0",
        "exorcist": "0.4.0",
        "express": "^4.14.0",
        "mocha": "^2.4.5",
        "mongoreporter": "^0.2.1",
        "request": "^2.79.0",
        "uglify-es": "^3.0.0",
        "utf-8-validate": "^3.0.0"
    },
    "scripts": {
        "build": "npm run browserify  && npm run uglifyjs",
